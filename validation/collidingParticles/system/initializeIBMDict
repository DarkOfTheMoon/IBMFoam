FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      innializeIBMDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
particleSizeDistribution monodisperse;
pTypes (mono1 mono2);
coupled true;

moving true;
rotating false;
e 0.8;

flow
{
    UInf            (1 0 0);
    rho             rhoInf;
    rhoInf          1.0;
}

mono1
{
    particleShape   cylinder;
    volumeFraction  0.2;
    d               0.05;
    rho             100;
    nTheta          100;
    delta           0.003;

    randomVelocity true;
    velocityDistributionCoeffs
    {
        type gaussian;
        mean 0;
        variance 0.2;
    }
}
mono2
{
    particleShape   cylinder;
    volumeFraction  0.2;
    d               0.025;
    rho             100;
    nTheta          100;
    delta           0.003;

    randomVelocity true;
    velocityDistributionCoeffs
    {
        type gaussian;
        mean 0;
        variance 0.1;
    }
}

